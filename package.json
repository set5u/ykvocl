{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run lintfix && npm run genlicenseinfo && nuxt build",
    "dev": "nuxt dev",
    "generate": "npm run lintfix && npm run genlicenseinfo && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \"js,.ts,.vue\" --ignore-path .gitignore .",
    "lintfix": "prettier --write --list-different . && npm run lint -- --fix",
    "genlicenseinfo": "license-checker --json --customPath license-format.json --out public/license-info.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lintfix"
    }
  },
  "devDependencies": {
    "@formkit/auto-animate": "^0.8.0",
    "@nuxt/devtools": "latest",
    "@plotdb/pug-browser": "^0.0.3",
    "@prettier/plugin-pug": "^3.0.0",
    "@tsconfig/strictest": "^2.0.2",
    "@types/dompurify": "^3.0.4",
    "@types/pug": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vue/language-plugin-pug": "^1.8.19",
    "@vueuse/core": "^10.5.0",
    "autoprefixer": "^10.4.16",
    "balm-ui": "^10.23.2",
    "browserfs": "^1.4.3",
    "dompurify": "^3.0.6",
    "eslint": "^8.51.0",
    "eslint-config": "^0.3.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vue-pug": "^0.6.0",
    "highlight.js": "^11.9.0",
    "husky": "^8.0.3",
    "license-checker": "^25.0.1",
    "marked": "^9.1.2",
    "marked-extended-tables": "^1.0.7",
    "marked-footnote": "^1.0.1",
    "marked-highlight": "^2.0.6",
    "marked-katex-extension": "^4.0.2",
    "marked-linkify-it": "^3.1.4",
    "nuxt": "^3.8.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "pug": "^3.0.2",
    "sass": "^1.69.4",
    "stacktrace-js": "^2.0.2",
    "tailwindcss": "^3.3.3",
    "tlds": "^1.243.0",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.2",
    "vue-eslint-parser-template-tokenizer-pug": "^0.4.10",
    "vue-router": "^4.2.5"
  }
}
